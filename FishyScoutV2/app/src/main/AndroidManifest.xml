<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ningning.msetscoutapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Input"
            android:label="@string/title_activity_input"
            android:theme="@style/AppTheme.NoActionBar" />
        <fragment
            android:name=".Autonomous"
            android:label="@string/title_activity_autonomous"
            android:id="@+id/autoFrag"
            android:theme="@style/AppTheme.NoActionBar" />
        <fragment
            android:name=".Teleop"
            android:label="@string/title_activity_teleop"
            android:id="@+id/teleFrag"
            android:theme="@style/AppTheme.NoActionBar" />
        <fragment
            android:name=".PostMatch"
            android:label="@string/title_activity_post_match"
            android:id="@+id/postFrag"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Confirmation"
            android:label="@string/title_activity_confirmation"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
